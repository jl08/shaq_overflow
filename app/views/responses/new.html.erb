<h6>Create a new response</h6>
<%= form_for [@question,@response] , html: {id:"new_response"} do |f| %>
<%= f.hidden_field :respondable_id, value: params[:question_id] %>
<%= f.hidden_field :respondable_type, value: "Question" %>
  <%= f.label :body %>
  <%= f.text_field :body %>
  <%= f.submit "Post response!" %>
<% end %>
